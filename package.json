{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist-server/app.js",
  "scripts": {
    "start": "npm run dev",
    "watch:dev": "nodemon",
    "dev": "NODE_ENV=development npm-run-all build server",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/app.js",
    "clean": "rimraf dist-server",
    "transpile": "babel ./src --out-dir dist-server --extensions .ts,.js,.tsx,.jsx --presets=@babel/typescript"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/multer": "^1.4.9",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "prettier": "^2.8.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "ext": "ts",
    "ignore": [
      "dist-server/*",
      "**/__tests__/**",
      "*.test.ts",
      "*.spec.ts"
    ]
  }
}
